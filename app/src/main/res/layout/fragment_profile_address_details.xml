<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileAddressDetailsVM" type="com.shopyyy.app.modules.profileaddressdetails.data.viewmodel.ProfileAddressDetailsVM"/>
    </data>
    <androidx.core.widget.NestedScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnaddAnotherAddress"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnaddresscounter"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAddressCounter"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{profileAddressDetailsVM.profileAddressDetailsModel.txtAddressCounter}"
                    style="@style/txtLatomedium13_1" tools:text="@string/lbl_address_1"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_50_radius_8"
                    android:ellipsize="end"
                    android:fontFamily="@font/lato"
                    android:hint="@string/lbl_enter_address"
                    android:id="@+id/etGroup5627"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={profileAddressDetailsVM.profileAddressDetailsModel.etGroup5627Value}"
                    android:textColor="@color/bluegray_400"
                    android:textColorHint="@color/bluegray_400"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnaddresscounterOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAddressCounterOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{profileAddressDetailsVM.profileAddressDetailsModel.txtAddressCounterOne}"
                    style="@style/txtLatomedium13_1" tools:text="@string/lbl_address_2"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_50_radius_8"
                    android:ellipsize="end"
                    android:fontFamily="@font/lato"
                    android:hint="@string/lbl_enter_address"
                    android:id="@+id/etGroup5627One"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={profileAddressDetailsVM.profileAddressDetailsModel.etGroup5627OneValue}"
                    android:textColor="@color/bluegray_400"
                    android:textColorHint="@color/bluegray_400"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumncity"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCity"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{profileAddressDetailsVM.profileAddressDetailsModel.txtCity}"
                    style="@style/txtLatomedium13_1" tools:text="@string/lbl_city"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_50_radius_8"
                    android:ellipsize="end"
                    android:fontFamily="@font/lato"
                    android:hint="@string/lbl_enter_your_city"
                    android:id="@+id/etGroup5627Two"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={profileAddressDetailsVM.profileAddressDetailsModel.etGroup5627TwoValue}"
                    android:textColor="@color/bluegray_400"
                    android:textColorHint="@color/bluegray_400"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnpostalcode"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPostalCode"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{profileAddressDetailsVM.profileAddressDetailsModel.txtPostalCode}"
                    style="@style/txtLatomedium13_1" tools:text="@string/lbl_postal_code"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_50_radius_8"
                    android:ellipsize="end"
                    android:fontFamily="@font/lato"
                    android:hint="@string/msg_enter_postal_co"
                    android:id="@+id/etGroup5627Three"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={profileAddressDetailsVM.profileAddressDetailsModel.etGroup5627ThreeValue}"
                    android:textColor="@color/bluegray_400"
                    android:textColorHint="@color/bluegray_400"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnphonenumber"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPhonenumber"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{profileAddressDetailsVM.profileAddressDetailsModel.txtPhonenumber}"
                    style="@style/txtLatomedium13_1" tools:text="@string/lbl_phone_number"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_50_radius_8"
                    android:ellipsize="end"
                    android:fontFamily="@font/lato"
                    android:hint="@string/msg_enter_phone_num"
                    android:id="@+id/etGroup5627Four"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={profileAddressDetailsVM.profileAddressDetailsModel.etGroup5627FourValue}"
                    android:textColor="@color/bluegray_400"
                    android:textColorHint="@color/bluegray_400"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_border_indigo_a200_radius_8"
                android:ellipsize="end" android:fontFamily="@font/lato"
                android:gravity="center"
                android:id="@+id/btnAddAnotherAddress"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_13pxv"
                android:singleLine="true"
                android:text="@string/msg_add_another_add"
                android:textAllCaps="true"
                android:textColor="@color/indigo_A200"
                android:textFontWeight="600"
                android:textSize="@dimen/_13pxh"
                android:textStyle="bold" tools:text="@string/msg_add_another_add"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCurrentaddress"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileAddressDetailsVM.profileAddressDetailsModel.txtCurrentaddress}"
                android:textAllCaps="true"
                style="@style/txtLatosemibold13_1" tools:text="@string/msg_current_address"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListhomeaddress"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:nestedScrollingEnabled="false"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_24pxh}" tools:itemCount="2" tools:listitem="@layout/row_listhomeaddress"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</layout>
