<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileProfileInfoVM" type="com.shopyyy.app.modules.profileprofileinfo.data.viewmodel.ProfileProfileInfoVM"/>
    </data>
    <ScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnsaveEdits"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumn"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_4">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnfirstname"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_4">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFirstName"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{profileProfileInfoVM.profileProfileInfoModel.txtFirstName}"
                        style="@style/txtLatomedium13_1" tools:text="@string/lbl_first_name"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_50_radius_6"
                        android:ellipsize="end"
                        android:fontFamily="@font/lato"
                        android:hint="@string/lbl_savannah"
                        android:id="@+id/etGroup5697"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.02"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_17pxv"
                        android:singleLine="true"
                        android:text="@={profileProfileInfoVM.profileProfileInfoModel.etGroup5697Value}"
                        android:textColor="@color/gray_902"
                        android:textColorHint="@color/gray_902"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_13pxh"
                        android:textStyle="normal" tools:ignore="Autofill"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnlastname"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_4">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLastName"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{profileProfileInfoVM.profileProfileInfoModel.txtLastName}"
                        style="@style/txtLatomedium13_1" tools:text="@string/lbl_last_name"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_50_radius_6"
                        android:ellipsize="end"
                        android:fontFamily="@font/lato"
                        android:hint="@string/lbl_nguyen"
                        android:id="@+id/etGroup5697One"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.02"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={profileProfileInfoVM.profileProfileInfoModel.etGroup5697OneValue}"
                        android:textColor="@color/gray_902"
                        android:textColorHint="@color/gray_902"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_13pxh"
                        android:textStyle="normal" tools:ignore="Autofill"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnemail"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_4">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmail"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{profileProfileInfoVM.profileProfileInfoModel.txtEmail}"
                        style="@style/txtLatomedium13_1" tools:text="@string/lbl_email_address"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_50_radius_6"
                        android:ellipsize="end"
                        android:fontFamily="@font/lato"
                        android:hint="@string/msg_savannahnguyen"
                        android:id="@+id/etEmailOne"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.02"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={profileProfileInfoVM.profileProfileInfoModel.etEmailOneValue}"
                        android:textColor="@color/gray_902"
                        android:textColorHint="@color/gray_902"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_13pxh"
                        android:textStyle="normal" tools:ignore="Autofill"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_indigo_a200_radius_8"
                android:ellipsize="end" android:fontFamily="@font/lato"
                android:gravity="center" android:id="@+id/btnSaveEdits"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_save_edits"
                android:textAllCaps="true"
                android:textColor="@color/white_A700"
                android:textFontWeight="600"
                android:textSize="@dimen/_13pxh"
                android:textStyle="bold" tools:text="@string/lbl_save_edits"/>
        </LinearLayout>
    </ScrollView>
</layout>
