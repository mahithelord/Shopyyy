<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listhomeaddressRowModel" type="com.shopyyy.app.modules.profileaddressdetails.data.model.ListhomeaddressRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearCurrentaddress"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_52cornerRadius">
        <LinearLayout android:id="@+id/linearRowhomeaddress"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtHomeaddress"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txtHomeaddress}"
                style="@style/txtLatomedium13" tools:text="@string/lbl_home_address"/>
            <LinearLayout android:id="@+id/linearRowedit"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEdit"
                    android:layout_height="@dimen/_13pxh"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_edit"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEditOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{listhomeaddressRowModel.txtEditOne}"
                    style="@style/txtLatomedium13_2" tools:text="@string/lbl_edit"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowaddresscounterTwo"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtAddressCounterTwo"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txtAddressCounterTwo}"
                style="@style/txtLatoregular13_1" tools:text="@string/lbl_address_1"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txtLanguage}"
                style="@style/txtLatoregular13" tools:text="@string/lbl_amoy_st_592"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowaddresscounterOne"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtAddressCounterOne"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txtAddressCounterOne}"
                style="@style/txtLatoregular13_1" tools:text="@string/lbl_address_2"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageOne"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txtLanguageOne}"
                style="@style/txtLatoregular13" tools:text="@string/lbl_amoy_st_592"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowcity"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtCityOne"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txtCityOne}"
                style="@style/txtLatoregular13_1" tools:text="@string/lbl_city"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLosAngeles"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txtLosAngeles}"
                style="@style/txtLatoregular13" tools:text="@string/lbl_los_angeles"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowpostalcode"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtPostalcodeOne"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txtPostalcodeOne}"
                style="@style/txtLatoregular13_1" tools:text="@string/lbl_postal_code2"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txt8829383"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{listhomeaddressRowModel.txt8829383}"
                style="@style/txtLatoregular13" tools:text="@string/lbl_8829383"/>
        </LinearLayout>
    </LinearLayout>
</layout>
